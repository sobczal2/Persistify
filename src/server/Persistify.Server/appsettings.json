{
    "Kestrel": {
        "EndpointDefaults": {
            "Protocols": "Http2"
        }
    },
    "Grpc": {
        "EnableDetailedErrors": true,
        "ResponseCompressionLevel": "Fastest",
        "ResponseCompressionAlgorithm": "gzip",
        "MaxReceiveMessageSize": 1048576,
        "MaxSendMessageSize": 1048576,
        "IgnoreUnknownServices": true
    },
    "Auth": {
        "ValidateIssuer": true,
        "ValidateAudience": true,
        "ValidateLifetime": true,
        "ValidateIssuerSigningKey": true,
        "ValidIssuer": "https://localhost:5001",
        "ValidAudience": "https://localhost:5001",
        "IssuerSigningKey": "This is a very long key that is used to sign the JWT tokens. It should be at least 16 characters long."
    },
    "Storage": {
        "DataPath": "/home/sobczal/Persistify_DataPath",
        "StorageType": "FileSystem",
        "SerializerType": "Protobuf"
    },
    "Logging": {
        "Default": "Information",
        "Seq": "http://localhost:5341"
    },
    "HostedServices": {
        "DocumentManagerIntervalSeconds": 10
    },
    "Cache": {
        "TemplateCacheCapacity": 10,
        "DocumentCacheCapacity": 10
    },
    "DataStructures": {
        "BTreeDegree": 10
    }
}
