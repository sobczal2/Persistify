{
    "Kestrel": {
        "EndpointDefaults": {
            "Protocols": "Http2"
        }
    },
    "Grpc": {
        "EnableDetailedErrors": true,
        "ResponseCompressionLevel": "Fastest",
        "ResponseCompressionAlgorithm": "gzip",
        "MaxReceiveMessageSize": 1048576,
        "MaxSendMessageSize": 1048576,
        "IgnoreUnknownServices": true
    },
    "Storage": {
        "DataPath": "/home/sobczal/Persistify_DataPath",
        "StorageType": "FileSystem",
        "SerializerType": "Protobuf",
        "IdleFileTimeout": "00:00:10"
    },
    "Logging": {
        "Default": "Warning",
        "Seq": "http://localhost:5341"
    },
    "HostedServices": {
        "DocumentManagerIntervalSeconds": 10
    },
    "Cache": {
        "TemplateCacheCapacity": 10,
        "DocumentCacheCapacity": 10
    },
    "DataStructures": {
        "BTreeDegree": 10
    },
    "Repository": {
        "TemplateRepositorySectorSize": 512,
        "DocumentRepositorySectorSize": 512,
        "UserRepositorySectorSize": 512,
        "RefreshTokenRepositorySectorSize": 512
    },
    "Password": {
        "Algorithm": "Argon2",
        "Iterations": 4,
        "MemorySize": 32768,
        "Parallelism": 2,
        "SaltSize": 16,
        "HashSize": 32
    },
    "Token": {
        "Type": "Jwt",
        "Secret": "Super secret jwt key",
        "AccessTokenLifetime": "30:00:00",
        "RefreshTokenLifetime": "30.00:00:00",
        "RefreshTokenLength": 32
    },
    "Root": {
        "Username": "root",
        "Password": "root"
    }
}
